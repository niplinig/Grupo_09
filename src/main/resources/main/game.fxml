<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox fx:id="vBox" alignment="TOP_CENTER" fillWidth="false" maxHeight="640.0" maxWidth="360.0" minHeight="640.0" minWidth="360.0" prefHeight="640.0" prefWidth="360.0" spacing="50.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.GameController">
   <children>
      <HBox alignment="CENTER" maxHeight="100.0" maxWidth="360.0" minHeight="100.0" minWidth="360.0" prefHeight="100.0" prefWidth="360.0" spacing="50.0">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <children>
            <Text fx:id="player1Sign" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text-id" text="Player" textAlignment="CENTER">
               <font>
                  <Font name="Montserrat Regular" size="20.0" />
               </font>
            </Text>
            <Text fx:id="player2Sign" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text-id" text="Computer">
               <font>
                  <Font name="Montserrat Regular" size="20.0" />
               </font>
            </Text>
         </children>
      </HBox>
   </children>
</VBox>
