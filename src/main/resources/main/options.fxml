<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" maxHeight="640.0" maxWidth="360.0" minHeight="640.0" minWidth="360.0" prefHeight="640.0" prefWidth="360.0" spacing="50.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.OptionsController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Options" textAlignment="CENTER">
         <font>
            <Font name="Montserrat Regular" size="50.0" />
         </font>
      </Text>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Theme Selector" textAlignment="CENTER">
         <font>
            <Font size="25.0" />
         </font>
      </Text>
      <ComboBox fx:id="cbThemes" maxHeight="70.0" maxWidth="200.0" minHeight="70.0" minWidth="200.0" onContextMenuRequested="#cbThemesSelected" prefHeight="70.0" prefWidth="200.0" />
      <Button fx:id="bttnMain" alignment="CENTER" contentDisplay="CENTER" maxHeight="70.0" maxWidth="200.0" minHeight="70.0" minWidth="200.0" mnemonicParsing="false" onMouseClicked="#mainClicked" prefHeight="70.0" prefWidth="200.0" text="Main Menu" textAlignment="CENTER">
         <font>
            <Font name="Montserrat Regular" size="30.0" />
         </font>
         <VBox.margin>
            <Insets top="20.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
